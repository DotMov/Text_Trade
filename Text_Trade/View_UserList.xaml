<Window x:Class="Text_Trade.View_UserList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Text_Trade"
        mc:Ignorable="d"
        Title="User List" Height="400" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="./Pictures/Text_Trade_background_big3.jpg" />
    </Window.Background>
    <Grid>
        <Label x:Name="label" Content="User List" HorizontalAlignment="Left" Margin="24,10,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listBox_reportList" HorizontalAlignment="Left" Height="264" Margin="24,41,0,0" VerticalAlignment="Top" Width="247" Background="#FFF7F6F6" BorderBrush="#FFEFAC54"/>
        <Button x:Name="viewButton" Content="View User" HorizontalAlignment="Left" Margin="113,330,0,0" VerticalAlignment="Top" Width="84" Click="viewButton_Click"/>

    </Grid>
</Window>


