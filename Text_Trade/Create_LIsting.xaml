        <ComboBox x:Name="comboBox_condition" HorizontalAlignment="Left" Margin="298,148,0,0" VerticalAlignment="Top" Width="96">
            <ComboBoxItem Content="New"></ComboBoxItem>
            <ComboBoxItem Content="Like New"></ComboBoxItem>
            <ComboBoxItem Content="Great"></ComboBoxItem>
            <ComboBoxItem Content="Good"></ComboBoxItem>
            <ComboBoxItem Content="Acceptable"></ComboBoxItem>
        </ComboBox>
        <TextBox x:Name="textBox_cL" HorizontalAlignment="Left" Height="23" Margin="118,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
        <TextBox x:Name="textBox_price" HorizontalAlignment="Left" Height="23" Margin="298,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="96"/>
        <Label x:Name="label8" Content="Description:" HorizontalAlignment="Left" Margin="31,218,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_description" HorizontalAlignment="Left" Height="90" Margin="118,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="276"/>
        <Button x:Name="button_createListing" Content="Create Listing" HorizontalAlignment="Left" Margin="173,341,0,0" VerticalAlignment="Top" Width="103" Height="46" BorderThickness="2" FontSize="14">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ControlColorKey}}"/>
            </Button.Background>
        </Button>
        <Label x:Name="label9" Content="Enter all fields below to create a listing" HorizontalAlignment="Left" Margin="118,10,0,0" VerticalAlignment="Top" Width="276" FontSize="14"/>
